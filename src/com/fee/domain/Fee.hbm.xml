<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.fee.domain.Fee" table="addFee">
        <id name="payId">
             <generator class="native" />
        </id>
        <property name="Bid"/>          
        <property name="Roomid" />        
        <property name="ownerName"/>       
        <property name="payType" />           
        <property name="lastMonthInfo"/>        
        <property name="thisMonthInfo"/>         
        <property name="unitPrice"/>     
        <property name="monthPay"/>
           
    </class>
</hibernate-mapping>
